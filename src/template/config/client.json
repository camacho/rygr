{
  "root": "client",
  "src": {
    "root": "client/src",
    "html": "client/src/html",
    "styles": "client/src/stylesheets",
    "public": "client/src/public",
    "scripts": "client/src/scripts",
    "images": "client/src/images"
  },
  "sass": {
    "sourceMap": false,
    "imagePath": "client/src/images"
  },
  "scripts": {
    "sourceMap": false
  },
  "build": {
    "root": "client/build",
    "tmp": "client/tmp",
    "assets": "client/build/assets",
    "maps": "client/build/assets/_maps",
    "src": "client/build/assets/_src"
  },
  "environments": {
    "development": {
      "sass": {
        "sourceMap": true
      },
      "scripts": {
        "sourceMap": true
      }
    }
  }
}
